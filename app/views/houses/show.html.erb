<h2><%= @house.name %></h2>
<p><%= @house.address %></p>
<p><%= @house.capacity %></p>
<% if @house.photo.attached? %>
          <%= cl_image_tag @house.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
<% @house.rooms.each do |room| %>
  <p><%= room.number %></p>
  <% room.tickets.each do |ticket| %>
    <p><%= ticket.title %></p>
    <p><%= ticket.description %></p>
    <p><%= ticket.status %></p>
    <p><%= ticket.priority %></p>
  <% end %>
<% end %>

<%= link_to "Edit",
  edit_house_path(@house)%>

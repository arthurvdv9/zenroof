<% @tickets.each do |ticket| %>
  <div class="ticket">
      <strong>Title:<%= link_to "#{ticket.title}", ticket_path(ticket)%></strong>
      <strong>Description:</strong> <%=ticket.description %>
      <strong>Status:</strong> <%= ticket.status %>
      <strong>Priority:</strong> <%=ticket.priority %>
  </div>
<% end %>



<div class="container">
<%# the two buttons to change view %>
  <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item" role="presentation">
          <a class="nav-link active" id="tenant-tab" data-bs-toggle="tab" href="#tenant" role="tab" aria-controls="tenant" aria-selected="true">Tenant</a>
      </li>

      <li class="nav-item" role="presentation">
          <a class="nav-link" id="owner-tab" data-bs-toggle="tab" href="#owner" role="tab" aria-controls="owner" aria-selected="false">Owner</a>
      </li>

  </ul>

<%# what happens when you click on the %>

  <div class="tab-content" id="myTabContent">
    
  <%# tenant view %>
      <div class="tab-pane fade show active" id="tenant" role="tabpanel" aria-labelledby="tenant-tab">
          <%= render partial: "tickets/myTickets" %>
      </div>

      <%# owner view %>
      <div class="tab-pane fade" id="owner" role="tabpanel" aria-labelledby="owner-tab">
          <%# render the search bar here %>
          <%= render partial: "houses/houseList" %>
      </div>
  </div>

</div>
